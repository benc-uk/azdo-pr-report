<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>PR Report for <%= project %></title>
  <link rel="shortcut icon" href="icon.png" type="image/png">

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.1/css/bulma.min.css"
    integrity="sha256-WLKGWSIJYerRN8tbNGtXWVYnUM5wMJTXD8eG4NtGcDM=" crossorigin="anonymous">
</head>

<body>
  <section class="section" style="padding: 15px">
    <h1 class="title">Open PRs for project: <%= project %></h1>
  </section>

  <section class="section" style="padding: 15px">
    <% prs.forEach(function(pr) { %>
    <div class="box">
      <h3 class="title is-4"><a
          href="<%= orgUrl %>/<%= project %>/_git/<%= pr.repository.name %>/pullrequest/<%= pr.pullRequestId %>"
          target="_blank"><%= pr.title %> (<%= pr.pullRequestId %>)</a></h4>
        <h5 class="title is-5"><%= pr.repository.name %></h5>
        <div class="content">
          <%= pr.createdBy.displayName %><br>
          <%= pr.sourceRefName %>

        </div>
    </div>
    <% }); %>

  </section>
</body>

</html>